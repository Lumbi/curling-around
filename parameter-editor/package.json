{
  "name": "parameter-editor",
  "version": "1.0.0",
  "description": "A simple application to edit key-value parameter files",
  "scripts": {
    "build": "concurrently \"npm run build:electron\" \"npm run build:react\"",
    "build:electron": "webpack --mode=development -c webpack.electron.config.js",
    "build:react": "webpack --mode=development -c webpack.react.config.js",
    "serve": "webpack serve -c webpack.react.config.js --static dist",
    "start": "electron ./dist/electron.js"
  },
  "author": "Gabriel Lumbi",
  "license": "ISC",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@webpack-cli/generators": "^3.0.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "css-loader": "^6.7.4",
    "electron": "^24.3.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.8",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
